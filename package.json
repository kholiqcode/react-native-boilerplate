{
  "name": "boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "android": "react-native run-android",
    "android:run": "ENVFILE=.env.development react-native run-android",
    "android:run:local": "adb reverse tcp:5001 tcp:5001 && ENVFILE=.env.local react-native run-android",
    "android:run:demo": "ENVFILE=.env.demo react-native run-android",
    "android:run:staging": "ENVFILE=.env.staging react-native run-android",
    "android:run:release": "ENVFILE=.env.production react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "release": "standard-version",
    "test": "jest --verbose --coverage",
    "test:update": "jest --verbose --coverage --updateSnapshot",
    "test:watch": "jest --verbose --watch",
    "coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx src/",
    "lint:fix": "eslint --ext .tsx --ext .ts src/ --fix",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "typescript": "tsc --project tsconfig.json",
    "start:staging": "MY_ENV=development npx react-native start --reset-cache",
    "start:release": "MY_ENV=production npx react-native start --reset-cache"
  },
  "dependencies": {
    "@jest/globals": "^28.1.0",
    "@react-native-async-storage/async-storage": "^1.17.5",
    "@react-native-community/netinfo": "^8.3.0",
    "@react-native-firebase/app": "^14.11.0",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@react-navigation/stack": "^6.2.1",
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/react-native": "^9.1.0",
    "axios": "^0.27.2",
    "i18next": "^21.8.4",
    "native-base": "^3.4.6",
    "promise-polyfill": "^8.2.3",
    "react": "17.0.2",
    "react-i18next": "^11.16.9",
    "react-native": "0.68.2",
    "react-native-dotenv": "^3.3.1",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "^2.4.2",
    "react-native-reanimated": "^2.8.0",
    "react-native-restart": "^0.0.24",
    "react-native-safe-area-context": "^4.2.5",
    "react-native-screens": "^3.13.1",
    "react-native-svg": "^12.3.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@jest/types": "^28.1.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.24",
    "@types/react-native": "^0.67.3",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-logger": "^3.0.9",
    "@types/redux-persist": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "babel-jest": "^27.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "husky": "^8.0.1",
    "jest": "^27.0.0",
    "metro-react-native-babel-preset": "^0.67.0",
    "prettier": "^2.6.2",
    "react-native-flipper": "^0.146.1",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "^18.1.0",
    "redux-flipper": "^2.0.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "engines": {
    "node": ">=14",
    "yarn": ">= 1.22",
    "npm": "Please use Yarn"
  }
}
